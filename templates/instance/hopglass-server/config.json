{
  "receiver": {
    "receivers": [
      { "module": "announced",
        "config": {
          "target": {
            "ip": "ff02::1",
            "port": 1001
          },
          "port": 38141,
          "timings": {
            "base": 300,
            "multipliers": {
              "statistics": 10,
              "neighbours": 10
            }
          }
        },
        "overlay": false
      },
      { "module": "aliases",
        "config": {
          "file": "./aliases.json"
        },
        "overlay": true
      }
    ],
    "ifaces": [
      "bat-nef27grd"
    ],
    "storage": {
      "interval": 300,
      "file": "/var/local/hopglass-server/nef27grd/raw.json"
    },
    "purge": {
      "maxAge": 14,
      "interval": 86400
    }
  },
  "provider": {
    "offlineTime": 900,
    "metricsOfflineTime": 60,
    "ffapiPath": "./ffapi/"
  },
  "webserver": {
    "ip": "::",
    "port": 48141
  }
}
