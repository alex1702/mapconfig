{
  "dataPath": [
    "https://karte.ffdus.de/data/",
    "https://karte.neanderfunk.de/data/",
    "https://map.freifunk-iserlohn.de/data/",
    "https://map-altena.freifunk-mk.de/data/",
    "https://map-meinerzhagen.freifunk-mk.de/data/",
    "https://map.gl.wupper.ffrl.de/data/",
    "https://map-hemer.freifunk-mk.de/data/"
  ],
  "siteName": "Eulenfunk",
  "mapSigmaScale": 0.5,
  "showContact": true,
  "maxAge": 14,
  "mapLayers": [
    { "name": "OSM Standard",
      "url": "https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png",
      "config": {
        "type": "osm", 
        "attribution": "<a target=\"_blank\" href=\"http://www.openstreetmap.org/\">Karte hergestellt aus OpenStreetMap-Daten</a> | <a rel=\"license\" target=\"_blank\" href=\"http://opendatacommons.org/licenses/odbl/\">Open Database License (ODbL)</a>",
        "maxZoom": 19
      }
    },
    { "name": "MapQuest",
      "url": "https://otile{s}-s.mqcdn.com/tiles/1.0.0/osm/{z}/{x}/{y}.jpg",
      "config": {
        "subdomains": "1234",
        "type": "osm",
        "attribution": "Tiles &copy; <a href=\"https://www.mapquest.com/\" target=\"_blank\">MapQuest</a>, Data CC-BY-SA OpenStreetMap",
        "maxZoom": 18
      }
    },
    { "name": "OSM deutscher Stil",
      "url": "http://a.tile.openstreetmap.de/tiles/osmde/{z}/{x}/{y}.png",
      "config": {
        "type": "osm",
        "attribution": "<a target=\"_blank\" href=\"http://www.openstreetmap.org/\">Karte hergestellt aus OpenStreetMap-Daten</a> | <a rel=\"license\" target=\"_blank\" href=\"http://opendatacommons.org/licenses/odbl/\">Open Database License (ODbL)</a>",
        "maxZoom": 18
      }
    }
  ],
  "nodeInfos": [
    { "href": "https://map.eulenfunk.de/stats/dashboard/db/node-byid?var-nodeid={NODE_ID}",
      "thumbnail": "https://map.eulenfunk.de/stats/render/dashboard-solo/db/node-byid?panelId=1&fullscreen&theme=light&width=600&height=300&var-nodeid={NODE_ID}"
    },
    { "href": "https://map.eulenfunk.de/stats/dashboard/db/node-byid?var-nodeid={NODE_ID}",
      "thumbnail": "https://map.eulenfunk.de/stats/render/dashboard-solo/db/node-byid?panelId=2fullscreen&theme=light&width=600&height=500&var-nodeid={NODE_ID}"
    },
    { "href": "https://map.eulenfunk.de/stats/dashboard/db/node-byid?var-nodeid={NODE_ID}",
      "thumbnail": "https://map.eulenfunk.de/stats/render/dashboard-solo/db/node-byid?panelId=3fullscreen&theme=light&width=600&height=200&var-nodeid={NODE_ID}"
    }
  ],
  "globalInfos": [
    { "href": "https://map.eulenfunk.de/stats/dashboard/db/global",
      "thumbnail": "https://map.eulenfunk.de/stats/render/dashboard-solo/db/global?panelId=1&fullscreen&theme=light&width=800&height=600"
    },
    { "href": "https://map.eulenfunk.de/stats/dashboard/db/global",
      "thumbnail": "https://map.eulenfunk.de/stats/render/dashboard-solo/db/global?panelId=8&fullscreen&theme=light&width=800&height=600"
    }
  ]
}
